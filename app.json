{
  "expo": {
    "name": "master-kg",
    "slug": "master-kg",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/circle.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/circle.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/circle.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/circle.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker"
    ]
  }
}